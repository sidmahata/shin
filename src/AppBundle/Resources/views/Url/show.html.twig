{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Url</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Userid</th>
                <td>{{ entity.userId }}</td>
            </tr>
            <tr>
                <th>Shortcode</th>
                <td>{{ entity.shortcode }}</td>
            </tr>
            <tr>
                <th>Alias</th>
                <td>{{ entity.alias }}</td>
            </tr>
            <tr>
                <th>Longurl</th>
                <td>{{ entity.longurl }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ entity.password }}</td>
            </tr>
            <tr>
                <th>Click</th>
                <td>{{ entity.click }}</td>
            </tr>
            <tr>
                <th>Metatitle</th>
                <td>{{ entity.metaTitle }}</td>
            </tr>
            <tr>
                <th>Metadescription</th>
                <td>{{ entity.metaDescription }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('url') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('url_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
